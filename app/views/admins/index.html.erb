<h1>Admin panel</h1>


<table class="table">
  <thead>
    <tr>
      <th scope="col">id</th>
      <th scope="col">login</th>
      <th scope="col">name</th>
      <th scope="col">Зарегистрирован</th>
    </tr>
  </thead>
<tbody>
<% @users.each do |u| %>

    <tr>
      <th scope="row"><%= u.id %></th>
      <td><%= u.login %></td>
      <td><%= u.name %></td>
      <td><%= u.created_at %></td>
    </tr>

<% end %>
</tbody>
</table>